<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    
    <system:String x:Key="SettingsBackgroundImageLabel">Hintergrund Bild</system:String>
    <system:String x:Key="SettingsBackgroundImageDescription">Wähle ein Hintergrundbild, dass du verwänden willst.</system:String>
    <system:String x:Key="SettingsBackgroundImageSelectWindowTitle">Wähle ein Hintergrundbild</system:String>
    <system:String x:Key="SettingsKeepEditorBackgroundLabel">Editor-Hintergrund beibehalten</system:String>
    <system:String x:Key="SettingsKeepEditorBackgroundDescription">Wenn aktiviert, wird das Editor-Feld, indem Code geschrieben wird, nicht transparent.</system:String>
    <system:String x:Key="SettingsBackgroundOpacityLabel">Hintergrund Transparenz</system:String>
    <system:String x:Key="SettingsBackgroundOpacityDescription">Setzt, wie durchsichtig das Editor-Feld und andere Oberflächen sind.</system:String>
    <system:String x:Key="SettingsBackgroundBlurLabel">Hintergrund Schärfe</system:String>
    <system:String x:Key="SettingsBackgroundBlurDescription">Definiert, wie scharf/unscharf das Hintergrundbild ist.</system:String>

</ResourceDictionary>